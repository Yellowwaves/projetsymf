{% extends 'base.html.twig' %}

{% block title %}Seismes{% endblock %}

{% block body %}
{% include 'entete.html.twig' %}
    <h1>Seismes</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ seisme.id }}</td>
            </tr>
            <tr>
                <th>Instant</th>
                <td>{{ seisme.instant ? seisme.instant|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Lat</th>
                <td>{{ seisme.lat }}</td>
            </tr>
            <tr>
                <th>Lon</th>
                <td>{{ seisme.lon }}</td>
            </tr>
            <tr>
                <th>Pays</th>
                <td>{{ seisme.pays }}</td>
            </tr>
            <tr>
                <th>Mag</th>
                <td>{{ seisme.mag }}</td>
            </tr>
            <tr>
                <th>Profondeur</th>
                <td>{{ seisme.profondeur }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_seismes_index') }}">back to list</a>

    <a href="{{ path('app_seismes_edit', {'id': seisme.id}) }}">edit</a>

    {{ include('seismes/_delete_form.html.twig') }}
    {% include 'footer.html.twig' %}

{% endblock %}
